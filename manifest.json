{
  "manifest_version": 2,
  "name": "Missouri S&T Catalog Assist",
  "version": "0.7",

  "content_scripts": [
    {
      "matches": [
        "http://catalog.mst.edu/*",
        "https://catalog.mst.edu/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "web_accessible_resources": [
    "courses.json"
  ]
}